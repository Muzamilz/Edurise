# Render Production Environment Variables
VITE_API_BASE_URL=https://your-backend-api.onrender.com/api/v1
VITE_WS_BASE_URL=wss://your-backend-api.onrender.com/ws
VITE_APP_NAME=Edurise
VITE_APP_VERSION=1.0.0
VITE_DEBUG=false

# OAuth Configuration (replace with your production keys)
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GITHUB_CLIENT_ID=your-github-client-id

# Payment Configuration (replace with your production keys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-key
VITE_PAYPAL_CLIENT_ID=your-paypal-client-id

# Zoom Integration (replace with your production keys)
VITE_ZOOM_SDK_KEY=your-zoom-sdk-key
VITE_ZOOM_SDK_SECRET=your-zoom-sdk-secret

# Feature Flags
VITE_ENABLE_3D_VISUALIZATIONS=true
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_PWA=false